<div class='section'>
  <header><h3>All Venues</h3></header>
  <div class='listbox'>
    <ul>
      <% Venue.all.each do | venue | %>
        <li><a href='/venues/<%= venue.id %>'><%= venue.name %></a></li>
      <% end %>
    </ul>
  </div>
</div>

<div class='section'>
  <header><h3>Add a Venue</h3></header>
  <form action='/venues' method='post'>
    <input type='text' id='name' name='name'>
    <button type='submit'>Add</button>
  </form>
</div>
